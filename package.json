{
  "name": "eth.gallery",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "$(npm bin)/concurrently \"npm run contract:watch\" \"npm run client:watch\" \"npm run server:serve\"",
    "build": "npm run contract:migrate && npm run client:build",
    "server:serve": "$(npm bin)/nodemon",
    "client:build": "rm -rf build && $(npm bin)/webpack && cp src/client/index.html build/index.html && npm run client:copy-libflif ",
    "client:watch": "$(npm bin)/watch \"npm run client:build\" src/client --wait=0",
    "client:copy-libflif": "cp -rf node_modules/libflif.js/lib/wrapper.js build/bundle/. && cp -rf node_modules/libflif.js/lib/worker.js build/bundle/. && cp -rf node_modules/libflif.js/lib/libflif.js build/bundle/. && cp -rf node_modules/libflif.js/lib/libflif.js.mem build/bundle/.",
    "eth:start": "ganache-cli --debug --account=\"0x519e6c7a4bb797f2bef6dff08c14063f978feb0e79ece37bc22cbc09abbada20,10000000000000000000000000000000000000000000\" --account=\"0x519e6c7a4bb797f2bef6dff08c14063f978feb0e79ece37bc22cbc09abbada21,10000000000000000000000000000000000000000000\"",
    "contract:build": "cd src/contract && $(npm bin)/truffle build",
    "contract:deploy": "cd src/contract && $(npm bin)/truffle deploy",
    "contract:migrate": "cd src/contract && rm -rf build && $(npm bin)/truffle migrate",
    "contract:console": "cd src/contract && $(npm bin)/truffle console",
    "contract:watch": "$(npm bin)/watch \"npm run build\" src/contract --wait=60 -p /build/g",
    "contract:test": "cd src/contract && $(npm bin)/truffle test"
  },
  "repository": {
    "type": "git",
    "url": "keybase://team/semag/eth.gallery"
  },
